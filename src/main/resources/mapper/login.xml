<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.lucy.login.LoginMapper">
    <insert id="userInsert" parameterType="com.example.lucy.login.LoginDTO">
        INSERT INTO user_table (userId, userPw) VALUES (#{userId}, #{userPw})
    </insert>

    <select id="userSelectList" resultType="List">
        SELECT * FROM user_table
    </select>
</mapper>